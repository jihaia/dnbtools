<!DOCTYPE html>
<html>
    <head>
        <title>D&B Tools Administration</title>
        <%= csrf_meta_tags %>

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

    <body class="fixed-header dashboard">

        <%= render partial: 'admins/core/navigator' %>
        <!-- START PAGE-CONTAINER -->
        <div class="page-container ">

            <%= render partial: 'admins/core/header' %>

            <!-- START PAGE CONTENT WRAPPER -->
            <div class="page-content-wrapper ">
                <%= yield %>
            </div>
            <!-- END PAGE CONTENT -->
            <%= render partial: 'admins/core/footer' %>
        </div>
        <!-- END PAGE CONTENT WRAPPER -->
    </div>
    <!-- END PAGE CONTAINER -->

    <%#= render partial: 'admins/core/sidebar' %>

    <%= render partial: 'admins/core/overlay' %>

</body>
</html>
